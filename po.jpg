  function myFunction3(event) {
    var x = document.getElementById("productSelect-"+ event).selectedIndex;
    
    var p = document.getElementById("productSelect-"+ event).value;
   
    
   var t = document.getElementById("size1-"+ event).innerHTML;
      var s = t.search("490");
      var p = t.search("470");
      var l = t.search("510");
      var q= t.search("580");
      var w= t.search("450");
    var z= t.search("440");
    var r =t.search("650");
    var v =t.search("540");
      
    
      if(s != -1) {
     document.getElementById("size2-"+ event).innerHTML = "Rs. 910.00";
      document.getElementById("size3-"+ event).innerHTML = "Rs. 1665.00";
    }
    
    if(p != -1) {
     document.getElementById("size2-"+ event).innerHTML = "Rs. 870.00";
      document.getElementById("size3-"+ event).innerHTML = "Rs. 1600.00";
    }
    if(l != -1) {
     document.getElementById("size2-"+ event).innerHTML = "Rs. 940.00";
      document.getElementById("size3-"+ event).innerHTML = "Rs. 1730.00";
    }
     if(q != -1) {
     document.getElementById("size2-"+ event).innerHTML = "Rs. 1070.00";
      document.getElementById("size3-"+ event).innerHTML = "Rs. 1970.00";
    }
     if(w != -1) {
     document.getElementById("size2-"+ event).innerHTML = "Rs. 830.00";
      document.getElementById("size3-"+ event).innerHTML = "Rs. 1530.00";
    }
     if(z != -1) {
     document.getElementById("size2-"+ event).innerHTML = "Rs. 810.00";
      document.getElementById("size3-"+ event).innerHTML = "Rs. 1500.00";
    }
    if(r != -1) {
     document.getElementById("size2-"+ event).innerHTML = "Rs. 1200.00";
      document.getElementById("size3-"+ event).innerHTML = "Rs. 2210.00";
    }
     if(v != -1) {
     document.getElementById("size2-"+ event).innerHTML = "Rs. 1000.00";
      document.getElementById("size3-"+ event).innerHTML = "Rs. 1840.00";
    }
    if(x == 0) {
     document.getElementById("size1-"+ event).style.display = "block";
      document.getElementById("size2-"+ event).style.display = "none";
      document.getElementById("size3-"+ event).style.display = "none";
      
      
      
      
    }
    
    if(x == 1) {
       document.getElementById("size1-"+ event).style.display = "none";
      document.getElementById("size2-"+ event).style.display = "block";
      document.getElementById("size3-"+ event).style.display = "none";
    } else if(x == 2) {
       document.getElementById("size1-"+ event).style.display = "none";
      document.getElementById("size2-"+ event).style.display = "none";
      document.getElementById("size3-"+ event).style.display = "block";
    }
}
  
  
